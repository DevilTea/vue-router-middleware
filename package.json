{
	"name": "@deviltea/vue-router-middleware",
	"type": "module",
	"version": "0.0.4",
	"packageManager": "pnpm@10.11.0",
	"description": "A simple router middleware system implemented by beforeEnter guard.",
	"author": "DevilTea <ch870814@gmail.com>",
	"license": "MIT",
	"homepage": "https://github.com/DevilTea/vue-router-middleware#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DevilTea/vue-router-middleware.git"
	},
	"bugs": "https://github.com/DevilTea/vue-router-middleware/issues",
	"keywords": [],
	"sideEffects": false,
	"exports": {
		".": {
			"types": {
				"import": "./dist/index.d.mts",
				"require": "./dist/index.d.cts",
				"default": "./dist/index.d.ts"
			},
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"dev": "unbuild --stub",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepublishOnly": "pnpm build",
		"release": "bumpp && pnpm publish",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"test:cov": "vitest --coverage",
		"typecheck": "tsc --noEmit --project tsconfig.test.json",
		"prepare": "simple-git-hooks"
	},
	"peerDependencies": {
		"vue": ">=3.0.0",
		"vue-router": ">=4.0.0"
	},
	"devDependencies": {
		"@deviltea/eslint-config": "4.0.0",
		"@deviltea/tsconfig": "0.0.7",
		"@types/node": "^20.10.1",
		"@vitest/coverage-v8": "3.1.4",
		"bumpp": "10.1.1",
		"eslint": "9.27.0",
		"lint-staged": "16.0.0",
		"rimraf": "6.0.1",
		"simple-git-hooks": "2.13.0",
		"tsx": "4.19.4",
		"typescript": "5.8.3",
		"unbuild": "3.5.0",
		"vite": "6.3.5",
		"vitest": "3.1.4",
		"vue": "3.5.14",
		"vue-router": "4.5.1"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*": "eslint --fix"
	},
	"publishConfig": {
		"access": "public"
	}
}
